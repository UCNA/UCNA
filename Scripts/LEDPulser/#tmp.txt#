  graph_465_moved_up[i] = new TGraphErrors(pulser_steps);
      double avgpulseratio = 0; 
      double nsteps = 0;
      for (int pulse = 0; pulse < pulser_steps; pulse++){
	double xUP,yUP;
	graph[UP][i]->GetPoint(pulse, xUP, yUP);
	double xDOWN, yDOWN;
	graph[DOWN][i]->GetPoint(pulser_steps - pulse - 1, xDOWN, yDOWN);
	cout << "X, Y, X, Y " << xUP << " " << yUP << " " << xDOWN << " " << yDOWN << endl;
	double pulseratio;
	if (yUP != 0){
	  pulseratio = yDOWN/yUP;
	  avgpulseratio += pulseratio;
	  nsteps++;
	}
      }
      avgpulseratio /= nsteps;
      cout << "AVG " << avgpulseratio << endl;